<!--?xml version="1.0" encoding="utf-8"?-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Include the database for backup -->
        <include domain="database" path="." />
        <!-- Exclude sensitive data -->
        <exclude domain="sharedpref" path="." />
    </cloud-backup>
    <device-transfer>
        <include domain="database" path="." />
        <exclude domain="cache" path="." />
    </device-transfer>
</data-extraction-rules>